{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobNew.schema.json",
    "type": "object",
    "properties": {
        "title": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30
        },
        "salary": {
          "type": "integer",
          "minLength": 1,
          "maxLength": 10
        },
        "equity": {
          "type": "boolean",
          "default": false
        }
      },
    "additionalProperties": false,
    "required": [
        "title",
        "salary",
        "equity"
        ]
}
